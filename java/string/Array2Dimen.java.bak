class Array2Dimen	{
	public static void main(String[] args) 	{
		int[][] arr = new int[3][4];
		// int형 데이터 12개를 저장할 수 있는 3행 4열 짜리 2차원 배열이 생성
		for (int i = 0; i < arr.length; i++)	{
			for (int j = 0; j < arr[i].length ; j++)	{
				arr[i][j] = i + j ;
			}
		}
		for (int i = 0; i < arr.length; i++)	{
			for (int j = 0; j < arr[i].length ; j++)	{
				System.out.print(arr[i][j] + "  ");
	}
		System.out.print;n();
}
}
}