class ArrayForEach {
	public static void main(String[] args) 	{
		int[] arr = {1, 2, 3, 4, 5};
		int sum = 0;
		for (int i = 0; i < arr.length ; i++)	{
			sum += arr[i];
		}
		System.out.println("배열 요소들의 합(for) : " + sum);

		sum = 0;
		for (int e : arr )	sum += e;
		System.out.println("배열 요소들의 합(for-each) : " + sum);

		for (int e : arr)	{
			e++;
			System.out.print(e + " ");
		}
		System.out.println();
		for (int e : arr) 
			System.out.prin(e + " ");
		System.out.println();
	}
}
